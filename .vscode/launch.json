    "version": "0.2.0",
    "configurations": [
        {
            "name": "Celery",
            "type": "python",
            "module": "celery",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd":"${workspaceFolder}/worker",
            "args": [
                "-A",
                "main",
                "worker",
                "--loglevel",
                "INFO",
                "--pool",
                "solo"
            ]
        }
    ]
}